{
  "name": "Ensime",
  "main": "./lib/ensime",
  "version": "0.41.0",
  "description": "Scala support via Ensime integration",
  "repository": "https://github.com/ensime/ensime-atom",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "scripts": {
    "lint": "eslint lib",
    "integration": "atom --dev --test spec; atom --dev --test spec-integration"
  },
  "dependencies": {
    "atom-message-panel": "~1.2.4",
    "atom-package-deps": "~3.0.9",
    "atom-space-pen-views": "^2.1.0",
    "bluebird": "^3.0.6",
    "diff": "^2.2.1",
    "dom-listener": "^0.1.2",
    "emissary": "1.x",
    "ensime-client": "~0.10.0",
    "fs-extra": "^0.26.5",
    "glob": "~6.0.4",
    "jquery": "^2",
    "lodash": "~4.3.0",
    "loglevel": "1.4.1",
    "recursive-readdir": "^1.3.0",
    "space-pen": "~5.1.2",
    "sub-atom": "~1.0.0",
    "temp": "^0.8.3",
    "vue": "1.0.0-csp"
  },
  "package-deps": [
    "hyperclick",
    "language-scala",
    "linter",
    "browser-plus",
    "intentions"
  ],
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "provideHyperclick"
      }
    },
    "intentions:list": {
      "versions": {
        "1.0.0": "provideIntentions"
      }
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeIndie"
      }
    }
  },
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "value": "unix",
      "level": "error"
    },
    "max_line_length": {
      "level": "warn",
      "value": 200
    }
  },
  "devDependencies": {
    "eslint": "^4.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  }
}
